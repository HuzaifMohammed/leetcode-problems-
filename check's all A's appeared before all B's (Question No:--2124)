class Solution {
    public boolean checkString(String s) {
        char[] ch = s.toCharArray();
        for(int i=0;i<ch.length-1;i++){
            for(int j=i+1;j<ch.length;j++){
                if(ch[i]<ch[j] || ch[i]==ch[j]){
                    continue;
                }else{
                    return false;
                }
            }
        }
        return true;
    }
}
